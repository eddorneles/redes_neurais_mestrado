function[] = create_sin_dataset()
    %cria matriz linha de 10000 elementos variando entre 0 e 2*pi
    interval_zero_2pi = 2 * pi * rand( 1, 10000 );
    save( 'dataset.mat');

    %divide o dataset
    [training_set, test_set] = divideblock( interval_zero_2pi, 0.7, 0.3, 0 );
    save('dataset_training');
    save('test_set');
end %function